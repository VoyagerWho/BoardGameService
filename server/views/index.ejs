<!DOCTYPE html>
<html lang="pl">
<%- include("./partials/head.ejs") %>

    <body onload="try { onBodyLoad(); } catch(err){ }">
        <div id="navigation">
            <%- include("./partials/nav.ejs") %>
        </div>
        <div id="backgroundBox">
            <div id="foregroundBox">
                &nbsp;
                <section id="pageHeader">
                    <%- include("./partials/header.ejs") %>
                </section>
                <section id="middle">
                    <% if (locals.middle) { %>
                        <%- include("./partials/" +(locals.middle)+".ejs") %>
                            <% } %>
                </section>
                <%- include("./partials/footer.ejs") %>
            </div>
        </div>
        <div id="extras">
            <img src="/static/images/Logo.png" alt="Logo" />
            <% if (locals.network) { %>
                <p>
                    <a href="http://jigsaw.w3.org/css-validator/check/referer">
                        <img style="border:0;width:88px;height:31px"
                            src="http://jigsaw.w3.org/css-validator/images/vcss-blue" alt="Poprawny CSS!" />
                    </a>
                </p>
                <% } %>
                    <% if (locals.game) { %>
                        <%- include("./partials/gamesparts/misc/users.ejs") %>
                            <% } %>
        </div>
    </body>

</html>